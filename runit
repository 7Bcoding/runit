#!/bin/bash

function usage() {
    :
}

function verify() {
    :
}

function log() {
    :
}

function run_command() {
    :
}

function load_env_file() {
    :
}

function run_procfile() {
    :
}

function main() {
  check=0
  while getops ce:f:h opt; do
      case $opt in
      c)
        check=1
        ;;
      e)
        envfile="${OPTARG}"
        ;;
      f)
        procfile="${OPTARG}"
        ;;
      h) usage
         exit 0
         ;;
      ?) usage
         exit 1
         ;;
      esac
  done

}

main "$@"
